
(define (problem dynamic_rescue_problem)
    (:domain dynamic_rescue)
    (:objects
        loc0 loc1 loc2 loc3 loc4 loc5 loc6 loc7 loc8 loc9 - location
    )
    (:init
        (robot_at loc0)
        (target_at loc5) (target_at loc7) (target_at loc2)
        (obstacle_at loc3) (obstacle_at loc3) (obstacle_at loc0) (obstacle_at loc9) (obstacle_at loc0)
    )
    (:goal (and (rescued loc2) (rescued loc5) (rescued loc7))
    )
)
